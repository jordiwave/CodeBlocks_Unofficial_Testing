<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="cbDiff Windows" />		<Option default_target="cbDiff" />
		<Option compiler="gcc" />		<Build>			<Target title="cbDiff">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/cbDiff" prefix_auto="0" extension_auto="1" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/cbDiff" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<ExtraCommands>					<!-- Create output directory if it does not exist -->					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Add before="cmd /c if exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip del ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip" />					<!-- Create zip file -->
					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip manifest.* *.xrc" />					<Add after="zip -rq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip images" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="cbDiff;" />		</VirtualTargets>		<Compiler>			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-mthreads" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="-m$(#CB_BUILD.OSBITS)" />			<!-- Add option="-fno-keep-inline-dllexport" \ -->			<Add option="-DEXPORT_FUNC" />			<Add option="-DBUILDING_PLUGIN" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-DWXUSINGDLL" />			<Add option="-DcbDEBUG" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add directory="./dtl" />			<Add directory="./src" />			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />			<Add directory="../../../include" />			<Add directory="../../../sdk/wxscintilla/include" />			<Add directory="../../../include/tinyxml" />		</Compiler>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS)/lib/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</ResourceCompiler>		<Linker>			<Add option="-Wl,--enable-auto-import" />			<Add option="-Wl,--no-undefined" />			<Add option="-mthreads" />			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />			<Add library="codeblocks" />			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />			<Add library="wxscintilla_cb" />		</Linker>		<Unit filename="cb_diff_toolbar.xrc" />		<Unit filename="manifest.xml" />		<Unit filename="manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="open.txt" />		<Unit filename="src/cbDiff.cpp" />		<Unit filename="src/cbDiff.h" />		<Unit filename="src/cbDiffConfigPanel.cpp" />		<Unit filename="src/cbDiffConfigPanel.h" />		<Unit filename="src/cbDiffCtrl.cpp" />		<Unit filename="src/cbDiffCtrl.h" />		<Unit filename="src/cbDiffEditor.cpp" />		<Unit filename="src/cbDiffEditor.h" />		<Unit filename="src/cbDiffMenu.cpp" />		<Unit filename="src/cbDiffMenu.h" />		<Unit filename="src/cbDiffSelectFiles.cpp" />		<Unit filename="src/cbDiffSelectFiles.h" />		<Unit filename="src/cbDiffUtils.cpp" />		<Unit filename="src/cbDiffUtils.h" />		<Unit filename="src/cbSideBySideCtrl.cpp" />		<Unit filename="src/cbSideBySideCtrl.h" />		<Unit filename="src/cbTableCtrl.cpp" />		<Unit filename="src/cbTableCtrl.h" />		<Unit filename="src/cbUnifiedCtrl.cpp" />		<Unit filename="src/cbUnifiedCtrl.h" />		<Unit filename="src/wxDiff.cpp" />		<Unit filename="src/wxDiff.h" />		<Unit filename="wxsmith/cbDiffConfigPanel.wxs" />		<Unit filename="wxsmith/cbDiffSelectFiles.wxs" />		<Extensions>			<lib_finder disable_auto="1" />			<wxsmith version="1">				<resources>					<wxPanel wxs="wxsmith/cbDiffConfigPanel.wxs" src="src/cbDiffConfigPanel.cpp" hdr="src/cbDiffConfigPanel.h" fwddecl="1" i18n="1" name="cbDiffConfigPanel" language="CPP" />					<wxDialog wxs="wxsmith/cbDiffSelectFiles.wxs" src="src/cbDiffSelectFiles.cpp" hdr="src/cbDiffSelectFiles.h" fwddecl="1" i18n="1" name="cbDiffSelectFiles" language="CPP" />				</resources>			</wxsmith>		</Extensions>	</Project></CodeBlocks_project_file>