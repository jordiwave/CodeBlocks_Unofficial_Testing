<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbDiff wx3.1.x (64 bit)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/cbDiff" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/cbDiff" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Add before="cmd /c if exist $(TARGET_OUTPUT_FILE) del $(TARGET_OUTPUT_FILE)" />
					<Add before="cmd /c if exist $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbDiff.zip del $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbDiff.zip" />

					<Add after="cmd /c if exist cbDiff.cbplugin del cbDiff.cbplugin" />
					<Add after="cmd /c if exist cbDiff.zip del cbDiff.zip" />
					
					<Add after="zip -j9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbDiff.zip manifest.xml" />
					<Add after="zip -rq9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbDiff.zip .\images" />
					<Add after="zip -j9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbDiff.zip *.xrc" />

					<!-- 
						<Add after="cmd /c copy $(TARGET_OUTPUT_FILE)" />
						<Add after="strip cbDiff.dll" />
						<Add after="zip -j9 cbDiff.zip manifest.xml" />
						<Add after="zip -rq9  cbDiff.zip .\images" />
						<Add after="zip -j9 cbDiff.zip *.xrc" />
						<Add after="zip -j9 cbDiff.cbplugin cbDiff.dll cbDiff.zip" />
						<Add after="cmd /c if exist cbDiff.zip del cbDiff.zip" />
						<Add after="cmd /c del cbDiff.dll" />
					-->
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<!-- Add option="-fno-keep-inline-dllexport" / -->
			<Add option="-DEXPORT_FUNC" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD)/include" />
			<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
			<Add directory="$(#CB_BUILD)/include/tinyxml" />
			<Add directory="dtl" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-Wl,--no-undefined" />
			<Add option="-mthreads" />
			<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add library="codeblocks" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="wxscintilla_cb" />
		</Linker>
		<Unit filename="cb_diff_toolbar.xrc" />
		<Unit filename="manifest.xml" />
		<Unit filename="open.txt" />
		<Unit filename="src/cbDiff.cpp" />
		<Unit filename="src/cbDiff.h" />
		<Unit filename="src/cbDiffConfigPanel.cpp" />
		<Unit filename="src/cbDiffConfigPanel.h" />
		<Unit filename="src/cbDiffCtrl.cpp" />
		<Unit filename="src/cbDiffCtrl.h" />
		<Unit filename="src/cbDiffEditor.cpp" />
		<Unit filename="src/cbDiffEditor.h" />
		<Unit filename="src/cbDiffMenu.cpp" />
		<Unit filename="src/cbDiffMenu.h" />
		<Unit filename="src/cbDiffSelectFiles.cpp" />
		<Unit filename="src/cbDiffSelectFiles.h" />
		<Unit filename="src/cbDiffUtils.cpp" />
		<Unit filename="src/cbDiffUtils.h" />
		<Unit filename="src/cbSideBySideCtrl.cpp" />
		<Unit filename="src/cbSideBySideCtrl.h" />
		<Unit filename="src/cbTableCtrl.cpp" />
		<Unit filename="src/cbTableCtrl.h" />
		<Unit filename="src/cbUnifiedCtrl.cpp" />
		<Unit filename="src/cbUnifiedCtrl.h" />
		<Unit filename="src/wxDiff.cpp" />
		<Unit filename="src/wxDiff.h" />
		<Unit filename="wxsmith/cbDiffConfigPanel.wxs" />
		<Unit filename="wxsmith/cbDiffSelectFiles.wxs" />
		<Extensions>
			<lib_finder disable_auto="1" />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/cbDiffConfigPanel.wxs" src="src/cbDiffConfigPanel.cpp" hdr="src/cbDiffConfigPanel.h" fwddecl="1" i18n="1" name="cbDiffConfigPanel" language="CPP" />
					<wxDialog wxs="wxsmith/cbDiffSelectFiles.wxs" src="src/cbDiffSelectFiles.cpp" hdr="src/cbDiffSelectFiles.h" fwddecl="1" i18n="1" name="cbDiffSelectFiles" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
