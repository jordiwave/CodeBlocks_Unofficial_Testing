<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="CBTortoiseSVN Windows" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="default">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/CBTortoiseSVN" prefix_auto="0" extension_auto="1" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/CBTortoiseSVN" />				<Option type="3" />				<Option compiler="gcc" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<ExtraCommands>					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Add before="cmd /c if exist $(TARGET_OUTPUT_FILE) del $(TARGET_OUTPUT_FILE)" />					<Add before="cmd /c if exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\CBTortoiseSVN.zip del ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\CBTortoiseSVN.zip" />					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\CBTortoiseSVN.zip manifest.xml" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="default;" />		</VirtualTargets>		<Compiler>			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-mthreads" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="-m$(#CB_BUILD.OSBITS)" />			<Add option="-fno-keep-inline-dllexport" />			<Add option="-DBUILDING_PLUGIN" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-DWXUSINGDLL" />			<Add option="-DcbDEBUG" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />			<Add directory="../../../include" />			<Add directory="../../../sdk/wxscintilla/include" />			<Add directory="../../../include/tinyxml" />			<Add directory="../../../include" />		</Compiler>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS)/lib/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</ResourceCompiler>		<Linker>			<Add option="-Wl,--enable-auto-import" />			<Add option="-Wl,--no-undefined" />			<Add option="-mthreads" />			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add library="codeblocks" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />		</Linker>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />		</ResourceCompiler>		<Unit filename="CBSvnPluginManager.cpp" />		<Unit filename="CBSvnPluginManager.h" />		<Unit filename="CBTortoiseSVN.cpp" />		<Unit filename="CBTortoiseSVN.h" />		<Unit filename="ConfigDialog.cpp" />		<Unit filename="ConfigDialog.h" />		<Unit filename="CustomConfigDialog.cpp" />		<Unit filename="CustomConfigDialog.h" />		<Unit filename="TwoPaneSelectionDialog.cpp" />		<Unit filename="TwoPaneSelectionDialog.h" />		<Unit filename="manifest.xml" />		<Unit filename="manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="menu.cpp" />		<Unit filename="menu.h" />		<Unit filename="utils.cpp" />		<Unit filename="utils.h" />		<Unit filename="wxsmith/ConfigDialog.wxs" />		<Unit filename="wxsmith/TwoPaneSelectionDialog.wxs" />		<Extensions>			<lib_finder disable_auto="1" />			<wxsmith version="1">				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />				<resources>					<wxDialog wxs="wxsmith/ConfigDialog.wxs" src="ConfigDialog.cpp" hdr="ConfigDialog.h" fwddecl="0" i18n="1" name="ConfigDialog" language="CPP" />					<wxDialog wxs="wxsmith/TwoPaneSelectionDialog.wxs" src="TwoPaneSelectionDialog.cpp" hdr="TwoPaneSelectionDialog.h" fwddecl="0" i18n="1" name="TwoPaneSelectionDialog" language="CPP" />					<wxDialog wxs="wxsmith/CustomConfigDialog.wxs" src="CustomConfigDialog.cpp" hdr="CustomConfigDialog.h" fwddecl="0" i18n="1" name="CustomConfigDialog" language="CPP" />				</resources>			</wxsmith>		</Extensions>	</Project></CodeBlocks_project_file>