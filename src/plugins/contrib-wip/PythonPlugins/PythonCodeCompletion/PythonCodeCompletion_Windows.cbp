<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Python_CodeCompletion Windows" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="PythonCodeCompletion">
				<Option output="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/PythonCodeCompletion" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/PythonCodeCompletion" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p Debug" />
				<Option host_application="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add directory="../XmlRpcEmbedder/XMLRPC/include" />
					<Add directory="../XmlRpcEmbedder" />
				</Compiler>
				<Linker>
					<Add library="XmlRpcEmbedder" />
				</Linker>
				<ExtraCommands>
					<!-- Create output directory -->
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Mode before="always" />

					<!-- Create the zip file -->
					<Add after="cmd /c if exist ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\PythonCodeCompletion.zip   del ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\PythonCodeCompletion.zip" />
					<Add after="cmd /c &quot;cd resources &amp; zip -jq9 ..\..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\PythonCodeCompletion.zip manifest.* *.xrc &quot;" />
					<Add after="cmd /c &quot;cd resources &amp; zip -rq9 ..\..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\PythonCodeCompletion.zip images &quot;" />

					<!-- Copy other files -->
					<Add after="cmd /c xcopy /y python\python_completion_server.py ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\codeblocks\python\" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="PythonCodeCompletion;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DcbDEBUG" />
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-DNOPCH" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-D_WINDOWS" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add directory="../../../../include" />
			<Add directory="../../../../include/tinyxml" />
			<Add directory="../../../../sdk/wxscintilla/include" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</Compiler>
		<Linker>
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
		</Linker>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</ResourceCompiler>
		<Unit filename="PythonCodeCompletion.cpp" />
		<Unit filename="PythonCodeCompletion.h" />
		<Unit filename="resources/manifest.xml" />
		<Unit filename="resources/manifest.rc" >
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="python/jedi_example.py" />
		<Unit filename="python/python_completion_server.py" />
		<Unit filename="python/stdlib_parser.py" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
