<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="Python_CodeCompletion Unix_Macos" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="PythonCodeCompletion">				<Option output="$(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/PythonCodeCompletion" prefix_auto="1" extension_auto="1" />				<Option working_dir="$(TARGET_OUTPUT_DIR)" />				<Option object_output="$(PROJECT_DIR)../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/PythonCodeCompletion" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p Debug" />				<Option host_application="$(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks" />				<Option run_host_application_in_terminal="0" />				<Compiler>					<Add directory="../XmlRpcEmbedder/XMLRPC/include" />					<Add directory="../XmlRpcEmbedder" />				</Compiler>				<Linker>					<Add library="XmlRpcEmbedder" />				</Linker>				<ExtraCommands>					<!-- Create output directory if it does not exist -->					<Add before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />					<Mode before="always" />					<!-- Create the zip file -->					<Add after="if [ -f $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonCodeCompletion.zip ] ; then rm $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonCodeCompletion.zip; fi" />					<Add after="cd resources &amp;&amp; zip -jq9 $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonCodeCompletion.zip manifest.* *.xrc" />					<Add after="cd resources &amp;&amp; zip -rq9 $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonCodeCompletion.zip images" />					<!-- copy other files  -->					<Add after="cp ./python/python_completion_server.py $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/python &gt; $CMD_NULL 2&gt; $CMD_NULL" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="PythonCodeCompletion;" />		</VirtualTargets>		<Compiler>			<Add option="-DcbDEBUG" />			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="-m$(#CB_BUILD.OSBITS)" />			<Add option="-fPIC" />			<Add option="-DNOPCH" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-Winvalid-pch" />			<Add option="-DWXUSINGDLL" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-DcbDEBUG" />			<Add directory="../../../../include" />			<Add directory="../../../../include/tinyxml" />			<Add directory="../../../../sdk/wxscintilla/include" />			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />		</Compiler>		<Linker>			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add library="codeblocks" />		</Linker>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</ResourceCompiler>		<Unit filename="PythonCodeCompletion.cpp" />		<Unit filename="PythonCodeCompletion.h" />		<Unit filename="resources/manifest.xml" />		<Unit filename="resources/manifest.rc" >			<Option compilerVar="WINDRES" />			<Option compile="0" />			<Option link="0" />		</Unit>		<Unit filename="python/jedi_example.py" />		<Unit filename="python/python_completion_server.py" />		<Unit filename="python/stdlib_parser.py" />	</Project></CodeBlocks_project_file>