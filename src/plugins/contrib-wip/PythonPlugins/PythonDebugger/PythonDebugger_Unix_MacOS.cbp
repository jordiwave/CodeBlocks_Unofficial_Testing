<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Python_Debugger Unix_Macos" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="PythonDebugger">
				<Option output="$(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/PythonDebugger" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="$(PROJECT_DIR)../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/PythonDebugger" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose" />
				<Option host_application="$(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<!-- Create output directory if it does not exist -->
					<Add before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />
					<Mode before="always" />

					<!-- Create the zip file -->
					<Add after="if [ -f $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonDebugger.zip ] ; then rm $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonDebugger.zip; fi" />
					<Add after="zip -qj9 $(PROJECT_DIR)../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/PythonDebugger.zip resources/manifest.xml resources/*.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="PythonDebugger;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DcbDEBUG" />
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-fPIC" />
			<Add option="-DNOPCH" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../../include" />
			<Add directory="../../../../include/tinyxml" />
			<Add directory="../../../../sdk/wxscintilla/include" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add library="codeblocks" />
		</Linker>
		<Unit filename="PythonDebugger.cpp" />
		<Unit filename="PythonDebugger.h" />
		<Unit filename="Resources/debugger_options_dlg.xrc" />
		<Unit filename="debuggeroptionsdlg.cpp" />
		<Unit filename="debuggeroptionsdlg.h" />
		<Unit filename="resources/manifest.xml" />
		<Unit filename="resources/manifest.rc" >
			<Option compilerVar="WINDRES" />
			<Option compile="0" />
			<Option link="0" />
		</Unit>
	</Project>
</CodeBlocks_project_file>
