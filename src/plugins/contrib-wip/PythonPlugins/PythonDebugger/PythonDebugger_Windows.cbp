<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="Python_Debugger Windows" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="PythonDebugger">				<Option output="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/PythonDebugger" prefix_auto="0" extension_auto="1" />				<Option working_dir="$(TARGET_OUTPUT_DIR)" />				<Option object_output="../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/PythonDebugger" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose" />				<Option host_application="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<ExtraCommands>					<!-- Create output directory -->					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<!-- Create the zip file -->					<Add after="zip -jq9 ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\codeblocks\PythonDebugger.zip resources/manifest.xml resources/*.xrc" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="PythonDebugger;" />		</VirtualTargets>		<Compiler>			<Add option="-DcbDEBUG" />			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="-m$(#CB_BUILD.OSBITS)" />			<Add option="-DNOPCH" />			<Add option="-mthreads" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-Winvalid-pch" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-DWXUSINGDLL" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-D_WINDOWS" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add directory="../../../../include" />			<Add directory="../../../../include/tinyxml" />			<Add directory="../../../../sdk/wxscintilla/include" />			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</Compiler>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</ResourceCompiler>		<Linker>			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />			<Add library="codeblocks" />		</Linker>		<Unit filename="PythonDebugger.cpp" />		<Unit filename="PythonDebugger.h" />		<Unit filename="Resources/debugger_options_dlg.xrc" />		<Unit filename="debuggeroptionsdlg.cpp" />		<Unit filename="debuggeroptionsdlg.h" />		<Unit filename="resources/manifest.xml" />		<Unit filename="resources/manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>	</Project></CodeBlocks_project_file>