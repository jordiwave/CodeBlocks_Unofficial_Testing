<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="Python_Interpreter Windows" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="PythonInterpreter">				<Option output="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/PythonInterpreter" prefix_auto="1" extension_auto="1" />				<Option working_dir="$(TARGET_OUTPUT_DIR)" />				<Option object_output="../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/PythonInterpreter" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p debug" />				<Option host_application="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<Compiler>					<Add directory="../XmlRpcEmbedder/XMLRPC/include" />					<Add directory="../XmlRpcEmbedder" />				</Compiler>				<Linker>					<Add library="XmlRpcEmbedder" />				</Linker>				<ExtraCommands>					<!-- Create output directory -->					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Mode before="always" />					<!-- Create the zip file -->					<Add after="zip -jq9 PythonInterpreter.zip resources\manifest.xml" />					<Add after="xcopy /Q /y PythonInterpreter.zip ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\codeblocks\" />					<Add after="xcopy /Q /y python\interp.py ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\codeblocks\python\" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="PythonInterpreter;" />		</VirtualTargets>		<Compiler>			<Add option="-DcbDEBUG" />			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="-m$(#CB_BUILD.OSBITS)" />			<Add option="-DNOPCH" />			<Add option="-mthreads" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-Winvalid-pch" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-DWXUSINGDLL" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-D_WINDOWS" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add directory="../../../../include" />			<Add directory="../../../../include/tinyxml" />			<Add directory="../../../../sdk/wxscintilla/include" />			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />		</Compiler>		<ResourceCompiler>			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />		</ResourceCompiler>		<Linker>			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs std,aui,propgrid`" />			<Add library="codeblocks" />		</Linker>		<Unit filename="PythonInterpCtrl.cpp" />		<Unit filename="PythonInterpCtrl.h" />		<Unit filename="PythonInterpreter.cpp" />		<Unit filename="PythonInterpreter.h" />		<Unit filename="ShellCtrlBase.cpp" />		<Unit filename="ShellCtrlBase.h" />		<Unit filename="resources/manifest.xml" />		<Unit filename="resources/manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="python/interp.py" />		<Unit filename="python/interp_client.py" />		<Extensions>		</Extensions>	</Project></CodeBlocks_project_file>