<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="C::B Games MacOS" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/byogames" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/byogames" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-D__WXMAC__" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
			<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
			<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --libs`" />
			<!-- Add option="-Wl,--no-undefined" -->
			<Add library="libcodeblocks.a" />
			<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/byogames.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="byocbtris.cpp" />
		<Unit filename="byocbtris.h" />
		<Unit filename="byoconf.cpp" />
		<Unit filename="byoconf.h" />
		<Unit filename="byogame.cpp" />
		<Unit filename="byogame.h" />
		<Unit filename="byogamebase.cpp" />
		<Unit filename="byogamebase.h" />
		<Unit filename="byogames.cpp" />
		<Unit filename="byogames.h" />
		<Unit filename="byogameselect.cpp" />
		<Unit filename="byogameselect.h" />
		<Unit filename="byosnake.cpp" />
		<Unit filename="byosnake.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/byoConf.wxs" />
		<Unit filename="wxsmith/byoGameSelect.wxs" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/byoGameSelect.wxs" src="byogameselect.cpp" hdr="byogameselect.h" fwddecl="0" i18n="1" name="byoGameSelect" language="CPP" />
					<wxPanel wxs="wxsmith/byoConf.wxs" src="byoconf.cpp" hdr="byoconf.h" fwddecl="0" i18n="1" name="byoConf" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
