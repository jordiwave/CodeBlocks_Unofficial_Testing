<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Debugger DAP Plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debugger_DAP_Plugin">
				<Option output="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/debugger_dap" prefix_auto="0" extension_auto="1" />
				<Option working_dir="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="../../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/debugger_dap" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="-p debug -ns -ni --debug-log --multiple-instance " />
				<Option host_application="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add platforms="Windows" before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Add platforms="Windows" before="cmd /c if exist $(TARGET_OUTPUT_BASENAME).cbplugin del $(TARGET_OUTPUT_BASENAME).cbplugin" />
					<Add platforms="Windows" before="cmd /c if exist $(TARGET_OUTPUT_BASENAME).zip      del $(TARGET_OUTPUT_BASENAME).zip" />
					<Add platforms="Windows" before="cmd /c if exist $(TARGET_OUTPUT_BASENAME).dll      del $(TARGET_OUTPUT_BASENAME).dll" />
					<Add platforms="Windows" before="cmd /c if exist ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip del ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip" />
					<Add platforms="Windows" after="zip -jq9 ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip resources/manifest.xml resources/*.xrc" />
					
					<Add platforms="Unix;Mac;" before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />
					<Add platforms="Unix;Mac;" after="if [ -f $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/$(TARGET_OUTPUT_BASENAME).zip ] ; then rm $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/$(TARGET_OUTPUT_BASENAME).zip; fi" />
					<Add platforms="Unix;Mac;" after="if [ -f $(TARGET_OUTPUT_BASENAME).cbplugin ] ; then rm $(TARGET_OUTPUT_BASENAME).cbplugin ; fi" />
					<Add platforms="Unix;Mac;" after="if [ -f $(TARGET_OUTPUT_BASENAME).zip      ] ; then rm $(TARGET_OUTPUT_BASENAME).zip      ; fi" />
					<Add platforms="Unix;Mac;" after="if [ -f $(TARGET_OUTPUT_BASENAME).so       ] ; then rm $(TARGET_OUTPUT_BASENAME).so      ; fi" />
					<Add platforms="Unix;Mac;" after="zip -jq9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/$(TARGET_OUTPUT_BASENAME).zip resources/manifest.xml resources/*.xrc" />

					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Debugger_DAP_Plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-DcbDEBUG" />
			<Add platforms="Windows" option="-m$(#CB_BUILD.OSBITS)" />
			<Add platforms="Windows" option="-pipe" />
			<Add platforms="Windows" option="-fmessage-length=0" />
			<Add platforms="Windows" option="-fexceptions" />
			<Add platforms="Windows" option="-mthreads" />
			<Add platforms="Windows" option="-fno-keep-inline-dllexport" />
			<Add platforms="Windows" option="-DHAVE_W32API_H" />
			<Add platforms="Windows" option="-D__WXMSW__" />
			<Add platforms="Windows" option="-DwxUSE_UNICODE" />
			<Add platforms="Windows" option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add platforms="Windows" directory="$(#WXWIDGETS.include)" />
			<Add platforms="Windows" directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />

			<Add platforms="Unix;Mac;" option="-fPIC" />

			<Add platforms="Unix" option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --cflags`" />
			<Add platforms="Unix" option="`pkg-config --cflags codeblocks`" />

			<Add platforms="Mac" option="-D__WXMAC__" />
			<Add platforms="Mac" option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --cflags`" />

			<Add directory="../DebugAdapterProtocol" />
			<Add directory="./src" />
			<Add directory="./tinyxml2" />
			<Add directory="../../../../include" />
			<Add directory="../../../../sdk/wxscintilla/include" />
			<Add directory="../../../../include/tinyxml" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</ResourceCompiler>
		<Linker>
			<Add platforms="Windows" option="-m$(#CB_BUILD.OSBITS)" />
			<Add platforms="Windows" option="-mthreads" />
			<Add platforms="Windows" option="-Wl,--enable-auto-image-base" />
			<Add platforms="Windows" option="-Wl,--add-stdcall-alias" />
			<Add platforms="Windows" option="-Wl,--enable-auto-import" />
			<Add platforms="Windows" option="-Wl,--no-undefined" />
			<Add platforms="Windows" library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add platforms="Windows" library="ws2_32" />
			<Add platforms="Windows" directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />

			<Add platforms="Unix" option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --libs std`" />
			<Add platforms="Unix" option="`/usr/local/bin/pkg-config --libs codeblocks`" />

			<Add platforms="Mac" option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --libs`" />
			<Add platforms="Mac" option="`pkg-config --libs codeblocks`" />

			<Add library="codeblocks" />
			<Add library="debugger_wxdap" />
			<Add directory="../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<Unit filename="resources/breakpoints_DAP_dlg.xrc" />
		<Unit filename="resources/debugger_DAP_options_dlg.xrc" />
		<Unit filename="resources/debugger_DAP_project_options_dlg.xrc" />
		<Unit filename="resources/edit_watches_DAP_dlg.xrc" />
		<Unit platforms="Windows" filename="resources/manifest.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="resources/manifest.xml" />
		<Unit filename="src/debugger_logger.cpp" />
		<Unit filename="src/debugger_logger.h" />
		<Unit filename="src/definitions.cpp" />
		<Unit filename="src/definitions.h" />
		<Unit filename="src/dlg_ProjectOptions.cpp" />
		<Unit filename="src/dlg_ProjectOptions.h" />
		<Unit filename="src/dlg_SettingsOptions.cpp" />
		<Unit filename="src/dlg_SettingsOptions.h" />
		<Unit filename="src/dlg_WatchEdit.cpp" />
		<Unit filename="src/dlg_WatchEdit.h" />
		<Unit filename="src/plugin.cpp" />
		<Unit filename="src/plugin.h" />
		<Unit filename="tinyxml2/tinyxml2.cpp" />
		<Unit filename="tinyxml2/tinyxml2.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
