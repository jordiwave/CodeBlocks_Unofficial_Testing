					<wxPanel wxs="wxsmith/ReopenEditorConfDLg.wxs" src="./ReopenEditorConfDLg.cpp" hdr="./ReopenEditorConfDLg.h" xrc="./ReopenEditorConfDLg.xrc" fwddecl="1" i18n="1" name="ReopenEditorConfDLg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
