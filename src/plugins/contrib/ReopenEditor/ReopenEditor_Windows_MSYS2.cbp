<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="ReopenEditor Windows" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="default">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/ReopenEditor" prefix_auto="0" extension_auto="1" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/ReopenEditor" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<Compiler>					<Add option="-Wall" />					<Add option="$(#CB_BUILD.CFLAGS)" />					<Add option="-pipe" />					<Add option="-mthreads" />					<Add option="-fmessage-length=0" />					<Add option="-fexceptions" />					<Add option="-DHAVE_W32API_H" />					<Add option="-D__WXMSW__" />					<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />					<Add option="-DWXUSINGDLL" />					<Add option="-DBUILDING_PLUGIN" />					<Add option="-DcbDEBUG" />					<Add option="-Winvalid-pch" />					<Add option="-DCB_PRECOMP" />					<Add option="-DwxUSE_UNICODE" />					<Add directory="../../../include" />					<Add directory="../../../include/tinyxml" />			<Add directory="../../../sdk/wxscintilla/include" />					<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />				</Compiler>				<Linker>					<Add option="-mthreads" />					<Add library="codeblocks" />					<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs std,aui,propgrid`" />					<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />				</Linker>				<ExtraCommands>					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\ReopenEditor.zip manifest.xml *.xrc" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="default;" />		</VirtualTargets>		<Compiler>			<Add option="-std=$(#CB_BUILD.CPP_STD)" />		</Compiler>		<ResourceCompiler>			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />		</ResourceCompiler>		<Unit filename="ReopenEditor.cpp" />		<Unit filename="ReopenEditor.h" />		<Unit filename="ReopenEditorConfDLg.cpp" />		<Unit filename="ReopenEditorConfDLg.h" />		<Unit filename="ReopenEditorListView.cpp" />		<Unit filename="ReopenEditorListView.h" />		<Unit filename="manifest.xml" />		<Unit filename="manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="wxsmith/ReopenEditorConfDLg.wxs" />		<Extensions>			<code_completion />			<envvars />			<debugger />			<lib_finder disable_auto="1" />			<wxsmith version="1">				<resources>					<wxPanel wxs="wxsmith/ReopenEditorConfDLg.wxs" src="./ReopenEditorConfDLg.cpp" hdr="./ReopenEditorConfDLg.h" xrc="./ReopenEditorConfDLg.xrc" fwddecl="1" i18n="1" name="ReopenEditorConfDLg" language="CPP" />				</resources>			</wxsmith>		</Extensions>	</Project></CodeBlocks_project_file>