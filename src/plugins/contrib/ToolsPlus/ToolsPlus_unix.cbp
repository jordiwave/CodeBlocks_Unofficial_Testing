<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Tools Plus Plugin Unix" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/ToolsPlus" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/ToolsPlus" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_BUILD.CFLAGS)" />
				</Compiler>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs std,aui`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -jq9 ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/ToolsPlus.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CmdConfigDialog.cpp" />
		<Unit filename="CmdConfigDialog.h" />
		<Unit filename="PipedProcessCtrl.cpp" />
		<Unit filename="PipedProcessCtrl.h" />
		<Unit filename="Resources/dialogs.fbp" />
		<Unit filename="ShellCtrlBase.cpp" />
		<Unit filename="ShellCtrlBase.h" />
		<Unit filename="ToolsPlus.cpp" />
		<Unit filename="ToolsPlus.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Unit filename="shellproperties.cpp" />
		<Unit filename="shellproperties.h" />
		<Unit filename="update" />
		<Unit filename="update.bat" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
