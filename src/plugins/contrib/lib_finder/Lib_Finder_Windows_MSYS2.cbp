<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="Library finder Windows" />		<Option pch_mode="2" />		<Option compiler="gcc" />		<Build>			<Target title="default">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/lib_finder.dll" prefix_auto="0" extension_auto="0" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/lib_finder" />				<Option external_deps="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/wxflatnotebook.dll;" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<Option projectResourceIncludeDirsRelation="0" />				<ExtraCommands>					<!-- Create output directory if it does not exist -->					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Add after="cmd /c if exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip del ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbSystemView.zip" />					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip manifest.xml" />					<Add before="cmd /c if not exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\lib_finder mkdir ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\lib_finder" />					<Add after="xcopy /Y /S /Q lib_finder\*.xml ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\lib_finder\ " />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="default;" />		</VirtualTargets>		<Compiler>			<Add option="-Wall" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-pipe" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-mthreads" />			<Add option="-DcbDEBUG" />			<Add option="-Winvalid-pch" />			<Add option="-DCB_PRECOMP" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add option="-DWXUSINGDLL" />			<Add option="-DwxUSE_UNICODE" />			<Add option="-DBUILDING_PLUGIN" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />			<Add directory="../../../include" />			<Add directory="../../../include/tinyxml" />			<Add directory="../../../sdk/wxscintilla/include" />			<Add directory="../../../include/scripting/include" />			<Add directory="../wxContribItems/wxFlatNotebook/include" />		</Compiler>		<ResourceCompiler>			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />		</ResourceCompiler>		<Linker>			<Add option="-Wl,--enable-auto-import" />			<Add option="-mthreads" />			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />			<Add library="codeblocks" />			<Add library="wxflatnotebook" />			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />		</Linker>		<Unit filename="defsdownloaddlg.cpp" />		<Unit filename="defsdownloaddlg.h" />		<Unit filename="dirlistdlg.cpp" />		<Unit filename="dirlistdlg.h" />		<Unit filename="headersdetectordlg.cpp" />		<Unit filename="headersdetectordlg.h" />		<Unit filename="lib_finder.cpp" />		<Unit filename="lib_finder.h" />		<Unit filename="lib_finder/codeblocks_sdk.xml" />		<Unit filename="lib_finder/glfw.xml" />		<Unit filename="lib_finder/readme.txt" />		<Unit filename="lib_finder/wxwidgets.xml" />		<Unit filename="librariesdlg.cpp" />		<Unit filename="librariesdlg.h" />		<Unit filename="librarydetectionconfig.h" />		<Unit filename="librarydetectionmanager.cpp" />		<Unit filename="librarydetectionmanager.h" />		<Unit filename="libraryresult.cpp" />		<Unit filename="libraryresult.h" />		<Unit filename="libselectdlg.cpp" />		<Unit filename="libselectdlg.h" />		<Unit filename="manifest.xml" />		<Unit filename="manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="pkgconfigmanager.cpp" />		<Unit filename="pkgconfigmanager.h" />		<Unit filename="processingdlg.cpp" />		<Unit filename="processingdlg.h" />		<Unit filename="projectconfiguration.cpp" />		<Unit filename="projectconfiguration.h" />		<Unit filename="projectconfigurationpanel.cpp" />		<Unit filename="projectconfigurationpanel.h" />		<Unit filename="projectmissinglibs.cpp" />		<Unit filename="projectmissinglibs.h" />		<Unit filename="resultmap.cpp" />		<Unit filename="resultmap.h" />		<Unit filename="webresourcesmanager.cpp" />		<Unit filename="webresourcesmanager.h" />		<Unit filename="wxsmith/DefsDownloadDlg.wxs" />		<Unit filename="wxsmith/DirListDlg.wxs" />		<Unit filename="wxsmith/HeadersDetectorDlg.wxs" />		<Unit filename="wxsmith/LibSelectDlg.wxs" />		<Unit filename="wxsmith/LibrariesDlg.wxs" />		<Unit filename="wxsmith/ProcessingDlg.wxs" />		<Unit filename="wxsmith/ProjectConfigurationPanel.wxs" />		<Unit filename="wxsmith/ProjectMissingLibs.wxs" />		<Extensions>			<code_completion />			<wxsmith version="1">				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />				<resources>					<wxScrollingDialog wxs="wxsmith/DirListDlg.wxs" src="dirlistdlg.cpp" hdr="dirlistdlg.h" fwddecl="1" i18n="1" name="DirListDlg" language="CPP" />					<wxScrollingDialog wxs="wxsmith/ProcessingDlg.wxs" src="processingdlg.cpp" hdr="processingdlg.h" fwddecl="1" i18n="1" name="ProcessingDlg" language="CPP" />					<wxPanel wxs="wxsmith/ProjectConfigurationPanel.wxs" src="projectconfigurationpanel.cpp" hdr="projectconfigurationpanel.h" fwddecl="1" i18n="1" name="ProjectConfigurationPanel" language="CPP" />					<wxScrollingDialog wxs="wxsmith/LibSelectDlg.wxs" src="libselectdlg.cpp" hdr="libselectdlg.h" fwddecl="1" i18n="1" name="LibSelectDlg" language="CPP" />					<wxScrollingDialog wxs="wxsmith/LibrariesDlg.wxs" src="librariesdlg.cpp" hdr="librariesdlg.h" fwddecl="1" i18n="1" name="LibrariesDlg" language="CPP" />					<wxScrollingDialog wxs="wxsmith/HeadersDetectorDlg.wxs" src="headersdetectordlg.cpp" hdr="headersdetectordlg.h" fwddecl="1" i18n="1" name="HeadersDetectorDlg" language="CPP" />					<wxScrollingDialog wxs="wxsmith/DefsDownloadDlg.wxs" src="defsdownloaddlg.cpp" hdr="defsdownloaddlg.h" fwddecl="1" i18n="1" name="DefsDownloadDlg" language="CPP" />					<wxScrollingDialog wxs="wxsmith/ProjectMissingLibs.wxs" src="projectmissinglibs.cpp" hdr="projectmissinglibs.h" fwddecl="1" i18n="1" name="ProjectMissingLibs" language="CPP" />				</resources>			</wxsmith>			<envvars />			<debugger />		</Extensions>	</Project></CodeBlocks_project_file>