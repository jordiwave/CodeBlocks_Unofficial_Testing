<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="DevPak Windows" />		<Option pch_mode="0" />		<Option default_target="default" />		<Option compiler="gcc" />		<Build>			<Target title="default">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/devpakupdater.dll" prefix_auto="0" extension_auto="0" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/devpak_plugin" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<Option projectResourceIncludeDirsRelation="0" />				<ExtraCommands>					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Add after="zip ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\devpakupdater.zip manifest.xml *.xrc" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="default;" />		</VirtualTargets>		<Compiler>			<Add option="-Wall" />			<Add option="-std=$(#CB_BUILD.CPP_STD)" />			<Add option="$(#CB_BUILD.CFLAGS)" />			<Add option="-mthreads" />			<Add option="-fexceptions" />			<Add option="-DBUILDING_PLUGIN" />			<Add option="-Winvalid-pch" />			<Add option="-DCB_PRECOMP" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add option="-DWXUSINGDLL" />			<Add option="-DcbDEBUG" />			<Add option="-DwxUSE_UNICODE" />			<Add directory="../../../include" />			<Add directory="../../../include/tinyxml" />			<Add directory="$(#WXWIDGETS.include)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />		</Compiler>		<ResourceCompiler>			<Add directory="$(#WXWIDGETS.include)" />		</ResourceCompiler>		<Linker>			<Add option="-mthreads" />			<Add library="codeblocks" />			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />			<Add library="bz2" />			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />			<Add option="-Wl,--enable-auto-import" />		</Linker>		<Unit filename="cbiniparser.cpp" />		<Unit filename="cbiniparser.h" />		<Unit filename="cbnetwork.cpp" />		<Unit filename="cbnetwork.h" />		<Unit filename="conf.cpp" />		<Unit filename="conf.h" />		<Unit filename="crc32.cpp" />		<Unit filename="crc32.h" />		<Unit filename="devpakinstaller.cpp" />		<Unit filename="devpakinstaller.h" />		<Unit filename="devpakupdater.cpp" />		<Unit filename="devpakupdater.h" />		<Unit filename="manifest.xml" />		<Unit filename="manifest.rc" >			<Option compilerVar="WINDRES" />		</Unit>		<Unit filename="mytar.cpp" />		<Unit filename="mytar.h" />		<Unit filename="update.xrc" />		<Unit filename="updatedlg.cpp" />		<Unit filename="updatedlg.h" />		<Extensions>			<code_completion />			<envvars />			<debugger />		</Extensions>	</Project></CodeBlocks_project_file>