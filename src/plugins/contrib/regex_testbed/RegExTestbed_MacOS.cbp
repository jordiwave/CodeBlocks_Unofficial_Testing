<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RegExTestbed MacOS" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/RegExTestbed" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/regex_testbed" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-fPIC" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="-pipe" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --cflags`" />
					<Add option="-DPIC" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add directory="$(#CB_BUILD.INCLUDE)" />
					<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
					<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --libs`" />
					<!-- Add option="-Wl,--no-undefined" -->
					<Add library="codeblocks" />
					<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/RegExTestbed.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-D__WXMAC__" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
		</Compiler>
		<Unit filename="RegExTestbed.cpp" />
		<Unit filename="RegExTestbed.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="regexdlg.cpp" />
		<Unit filename="regexdlg.h" />
		<Unit filename="regexdlg.xrc" />
		<Unit filename="wxsmith/RegExDlg.wxs" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/RegExDlg.wxs" src="regexdlg.cpp" hdr="regexdlg.h" xrc="regexdlg.xrc" fwddecl="0" i18n="1" name="RegExDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
