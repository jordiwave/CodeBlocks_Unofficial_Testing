<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="SymTab Unix" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/SymTab" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/symtab" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -jq9 ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SymTab.zip resources/manifest.xml resources/*.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="symtab.cpp" />
		<Unit filename="symtab.h" />
		<Unit filename="symtabconfig.cpp" />
		<Unit filename="symtabconfig.h" />
		<Unit filename="symtabexec.cpp" />
		<Unit filename="symtabexec.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
