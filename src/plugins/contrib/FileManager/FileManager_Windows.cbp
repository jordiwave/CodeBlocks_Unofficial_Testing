<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FileManager Plugin Windows" />
		<Option platforms="Windows;" />
		<Option compiler="gcc" />
		<Build>
			<Target title="FileManager">
				<Option output="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\FileManager" prefix_auto="0" extension_auto="1" />
				<Option object_output="..\..\..\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\$(TARGET_OUTPUT_BASENAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
					<Add option="-DcbDEBUG" />
					<Add option="-Winvalid-pch" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			        <Add directory="../../../include" />
			        <Add directory="../../../include/tinyxml" />
			        <Add directory="../../../sdk/wxscintilla/include" />
					<Add directory="$(#WXWIDGETS.include)" />
					<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#WXWIDGETS.include)" />
					<Add directory="$(#WXWIDGETS)/lib/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
				</ResourceCompiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
					<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
					<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<!-- Create output directory if it does not exist -->
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />

					<!-- Create plugin zip file -->
					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip manifest.xml Resources/*.xrc" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="FileManager;" />
		</VirtualTargets>
		<Unit filename="CommitBrowser.cpp" />
		<Unit filename="CommitBrowser.h" />
		<Unit filename="FileExplorer.cpp" />
		<Unit filename="FileExplorer.h" />
		<Unit filename="FileExplorerSettings.cpp" />
		<Unit filename="FileExplorerSettings.h" />
		<Unit filename="FileExplorerUpdater.cpp" />
		<Unit filename="FileExplorerUpdater.h" />
		<Unit filename="FileManager.cpp" />
		<Unit filename="FileManager.h" />
		<Unit filename="Resources/CommitBrowser.xrc" />
		<Unit filename="Resources/fileexplorersettings.fbp" />
		<Unit filename="Resources/fileexplorersettings.xrc" />
		<Unit filename="directorymonitor.cpp" />
		<Unit filename="directorymonitor.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="manifest.rc" >
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
