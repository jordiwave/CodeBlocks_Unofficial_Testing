<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ProjectOptionsManipulator Windows" />
		<Option pch_mode="2" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\ProjectOptionsManipulator" prefix_auto="0" extension_auto="1" />
				<Option working_dir="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="..\..\..\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\ProjectOptionsManipulator" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p debug" />
				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="codeblocks" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\ProjectOptionsManipulator.zip manifest.xml" />
		</ExtraCommands>
		<Unit filename="ProjectOptionsManipulator.cpp" />
		<Unit filename="ProjectOptionsManipulator.h" />
		<Unit filename="ProjectOptionsManipulatorDlg.cpp" />
		<Unit filename="ProjectOptionsManipulatorDlg.h" />
		<Unit filename="ProjectOptionsManipulatorResultDlg.cpp" />
		<Unit filename="ProjectOptionsManipulatorResultDlg.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="manifest.rc" >
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="wxsmith/ProjectOptionsManipulatorDlg.wxs" />
		<Unit filename="wxsmith/ProjectOptionsManipulatorResultDlg.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxDialog wxs="wxsmith/ProjectOptionsManipulatorDlg.wxs" src="ProjectOptionsManipulatorDlg.cpp" hdr="ProjectOptionsManipulatorDlg.h" fwddecl="1" i18n="1" name="ProjectOptionsManipulatorDlg" language="CPP" />
					<wxDialog wxs="wxsmith/ProjectOptionsManipulatorResultDlg.wxs" src="ProjectOptionsManipulatorResultDlg.cpp" hdr="ProjectOptionsManipulatorResultDlg.h" fwddecl="1" i18n="1" name="ProjectOptionsManipulatorResultDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
