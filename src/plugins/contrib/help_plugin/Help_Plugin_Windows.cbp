<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help Plugin Windows" />
		<Option pch_mode="0" />
		<Option default_target="help_plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="help_plugin">
				<Option output="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\help_plugin.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="$(#CB_BUILD)\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\help_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<!-- Create output directories -->
					<!-- Create output directory -->
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Add before="cmd /c if not exist $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\ mkdir $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\ " />
					<Mode before="always" />

					<!-- Create help_plugin.zip -->
					<Add after="zip -jq9  $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\help_plugin.zip manifest.xml *.xrc" />
					<Add after="zip -rq9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\help_plugin.zip .\images" />

					<!-- Copy file to docs directory -->
					<Add after="cmd /c if not exist $(#CB_BUILD)\..\windows_installer\Documentation xcopy /Y /Q index.ini                                           $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\ " />
					<Add after="cmd /c if     exist $(#CB_BUILD)\..\windows_installer\Documentation xcopy /Y /Q $(#CB_BUILD)\..\windows_installer\Documentation\*.* $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\ " />

					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
			<Add directory="$(#CB_BUILD.INCLUDE)\tinyxml" />
			<Add directory="$(#CB_BUILD)\sdk\wxscintilla\include" />
			<Add directory="$(#CB_BUILD.INCLUDE)\scripting\include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS)/lib/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</ResourceCompiler>
		<Linker>
            <Add option="-mthreads" />
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add option="-Wl,--enable-auto-import" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="bz2" />
			<Add library="z" />
			<Add library="codeblocks" />
			<Add directory="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
		</Linker>
		<Unit filename="HelpConfigDialog.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="HelpConfigDialog.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="MANFrame.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="MANFrame.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="defs.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="help_common.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="help_common.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="help_plugin.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="help_plugin.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="help_plugin.xrc">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="man2html.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="man2html.h">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="manifest.xml">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="manifest.rc" >
			<Option compilerVar="WINDRES" />
			<Option target="help_plugin" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
