<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ProjectExporter Windows" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="ProjectExporter">
				<Option output="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\ProjectExporter" prefix_auto="0" extension_auto="1" />
				<Option working_dir="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="$(#CB_BUILD)\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\ProjectExporter" />
				<Option type="3" />
				<Option host_application="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<!-- Create output directory -->
					<Add before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />
					<Mode before="always" />

					<Add after="if [ -f  $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/ProjectExporter.zip ] ; then rm $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/ProjectExporter.zip; fi;" />
					<Add after="zip -jq9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/ProjectExporter.zip manifest.xml PremakeDlg.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="ProjectExporter;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-fPIC" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --toolkit=gtk3 --cflags`" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
			<Add directory="$(#CB_BUILD)\sdk\wxscintilla\include" />
			<Add directory="$(#CB_BUILD.INCLUDE)\tinyxml" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --toolkit=gtk3 --libs std,aui,propgrid`" />
			<Add library="codeblocks" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="ole32" />
			<Add library="uuid" />
			<Add library="rpcrt4" />
			<Add directory="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<Unit filename="AutotoolsExporter.cpp" />
		<Unit filename="AutotoolsExporter.h" />
		<Unit filename="BakefileExporter.cpp" />
		<Unit filename="BakefileExporter.h" />
		<Unit filename="CMakeListsExporter.cpp" />
		<Unit filename="CMakeListsExporter.h" />
		<Unit filename="ExporterBase.cpp" />
		<Unit filename="ExporterBase.h" />
		<Unit filename="PremakeDlg.cpp" />
		<Unit filename="PremakeDlg.h" />
		<Unit filename="PremakeDlg.xrc" />
		<Unit filename="PremakeExporter.cpp" />
		<Unit filename="PremakeExporter.h" />
		<Unit filename="ProjectExporter.cpp" />
		<Unit filename="ProjectExporter.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="manifest.rc" >
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="wxsmith\PremakeDlg.wxs" />
		<Extensions>
			<wxsmith version="1">
				<resources>
					<wxScrollingDialog wxs="wxsmith/ExportDialog.wxs" src="ExportDialog.cpp" hdr="ExportDialog.h" xrc="ExportDialog.xrc" fwddecl="0" i18n="1" name="ExportDialog" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
