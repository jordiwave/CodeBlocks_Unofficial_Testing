<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EnvVars MacOS" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/envvars" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fexceptions" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../include/scripting/include" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -jq9 ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/envvars.zip manifest.xml *.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="envvars.cpp" />
		<Unit filename="envvars.h" />
		<Unit filename="envvars.xrc" />
		<Unit filename="envvars_cfgdlg.cpp" />
		<Unit filename="envvars_cfgdlg.h" />
		<Unit filename="envvars_common.cpp" />
		<Unit filename="envvars_common.h" />
		<Unit filename="envvars_prjoptdlg.cpp" />
		<Unit filename="envvars_prjoptdlg.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="project_settings.xrc" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
