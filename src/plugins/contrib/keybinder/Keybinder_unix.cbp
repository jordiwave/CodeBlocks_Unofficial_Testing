<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="KeyBinder Unix" />
		<Option compiler="gcc" />
		<Build>
			<Target title="KeyBinder_Plugin">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/keybinder" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/keybinder" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add directory="$(#CB_BUILD.INCLUDE)" />
					<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
					<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add library="libcodeblocks.a" />
					<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
				<ExtraCommands>
					<!-- Create output directory if it does not exist -->
					<Add before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />

					<Add after="zip -jq9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/keybinder.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="KeyBinder_Plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --cflags`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --libs`" />
		</Linker>
		<Unit filename="cJSON.cpp" />
		<Unit filename="cJSON.h" />
		<Unit filename="cbKeyConfigPanel.cpp" />
		<Unit filename="cbKeyConfigPanel.h" />
		<Unit filename="cbKeybinder.cpp" />
		<Unit filename="cbKeybinder.h" />
		<Unit filename="clKeyboardBindingConfig.cpp" />
		<Unit filename="clKeyboardBindingConfig.h" />
		<Unit filename="clKeyboardManager.cpp" />
		<Unit filename="clKeyboardManager.h" />
		<Unit filename="debugging.h" />
		<Unit filename="json_node.cpp" />
		<Unit filename="json_node.h" />
		<Unit filename="Keybinder.cpp" />
		<Unit filename="Keybinder.h" />
		<Unit filename="macros.h" />
		<Unit filename="menuutils.cpp" />
		<Unit filename="menuutils.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
