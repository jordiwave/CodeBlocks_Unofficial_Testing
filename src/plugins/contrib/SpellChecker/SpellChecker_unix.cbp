					<Add option="-DCB_PRECOMP" />
					<Add option="-DcbDEBUG" />
					<Add directory="wxspellchecker/include" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add option="-lwxSpellChecker" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
					<Add option="`$(#CB_BUILD.PKG_CONFIG_EXE) hunspell --libs`" />
					<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
                <ExtraCommands>
        			<!-- Create output directory -->
        			<Add before="if [ ! -d $(TARGET_OUTPUT_DIR) ] ; then mkdir -p $(TARGET_OUTPUT_DIR) ; fi" />
        			<Mode before="always" />

        			<!-- Create plugin zip file-->
                    <Add after="zip -jq9 ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker.zip manifest.xml SpellCheckSettingsPanel.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="wxSpellChecker;plugin;" />
		</VirtualTargets>
		<ExtraCommands>
			<!-- Create output directory -->
			<Add before="if [ ! -d ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker ] ; then mkdir -p ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker   ; fi" />
			<Add before="if [ -f ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker/OnlineSpellChecking.xml ] ; then rm -r ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker/* ; fi" />
			<Mode before="always" />

			<!-- Copy Spellchecker dictionary files -->
			<Add after="cp -f OnlineSpellChecking.xml ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker/" />
			<Add after="cp -rf dictionaries/*         ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/SpellChecker/" />

