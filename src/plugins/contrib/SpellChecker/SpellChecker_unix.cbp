				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option projectCompilerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
					<Add option="`$(#CB_BUILD.PKG_CONFIG_EXE) hunspell --cflags`" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-fPIC" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DHUNSPELL_STATIC" />
					<Add directory="wxspellchecker/include" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
					<Add option="`$(#CB_BUILD.PKG_CONFIG_EXE) hunspell --libs`" />
					<Add library="codeblocks" />
					<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
			</Target>
			<Target title="plugin">
				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/SpellChecker" prefix_auto="1" extension_auto="1" />
