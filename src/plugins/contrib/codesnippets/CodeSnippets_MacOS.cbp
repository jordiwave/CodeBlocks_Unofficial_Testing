<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CodeSnippets MacOS" />
		<Option default_target="CodeSnippets_Plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="CodeSnippets_Plugin">
				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/codesnippets" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/codesnippets/plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add before="if [ ! -d ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks ] ; then mkdir -p ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks ; fi" />
					<Add before="if [ ! -d ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/images/codesnippets ] ; then mkdir -p ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/images/codesnippets ; fi" />

					<!-- Create codesnippets.zip -->
					<Add after="if [ -f ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/codesnippets.zip ] ; then rm ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/codesnippets.zip ; fi" />
					<Add after="cd resources &amp;&amp; zip -jq9 ../../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/codesnippets.zip manifest.xml" />

					<!-- copy PNG files  -->
					<Add after="cp ./resources/*.png ../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/images/codesnippets/ &gt; /dev/null 2&gt; /dev/null" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="CodeSnippets_Plugin" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="." />
		</Compiler>
		<Linker>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs std,aui,propgrid`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<Unit filename="Makefile.am" />
		<Unit filename="codesnippets.cpp" />
		<Unit filename="codesnippets.h" />
		<Unit filename="codesnippetstreectrl.cpp" />
		<Unit filename="codesnippetstreectrl.h" />
		<Unit filename="codesnippetswindow.cpp" />
		<Unit filename="codesnippetswindow.h" />
		<Unit filename="menuidentifiers.h" />
		<Unit filename="resources/Makefile.am" />
		<Unit filename="resources/allsnippets.png" />
		<Unit filename="resources/category.png" />
		<Unit filename="resources/findf.png" />
		<Unit filename="resources/findfdisabled.png" />
		<Unit filename="resources/manifest.rc">
			<Option compile="0" />
			<Option link="0" />
		</Unit>
		<Unit filename="resources/manifest.xml" />
		<Unit filename="resources/options.png" />
		<Unit filename="resources/optionsdisabled.png" />
		<Unit filename="resources/snippet.png" />
		<Unit filename="resources/stop.png" />
		<Unit filename="resources/stopdisabled.png" />
		<Unit filename="settingsdlg.cpp" />
		<Unit filename="settingsdlg.h" />
		<Unit filename="settingsdlgform.cpp" />
		<Unit filename="settingsdlgform.h" />
		<Unit filename="snippetitemdata.cpp" />
		<Unit filename="snippetitemdata.h" />
		<Unit filename="snippetproperty.cpp" />
		<Unit filename="snippetproperty.h" />
		<Unit filename="snippetpropertyform.cpp" />
		<Unit filename="snippetpropertyform.h" />
		<Unit filename="snippetsconfig.cpp" />
		<Unit filename="snippetsconfig.h" />
		<Unit filename="snippetsimages.cpp" />
		<Unit filename="snippetsimages.h" />
		<Unit filename="version.cpp" />
		<Unit filename="version.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
