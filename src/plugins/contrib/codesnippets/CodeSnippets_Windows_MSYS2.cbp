<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CodeSnippets Windows" />
		<Option default_target="CodeSnippets_Plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="CodeSnippets_Plugin">
				<Option output="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\codesnippets.dll" prefix_auto="0" extension_auto="0" />
				<Option working_dir="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="..\..\..\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\codesnippets\plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Add before="cmd /c if not exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\codesnippets mkdir ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\codesnippets" />
					<!-- Create codesnippets.zip -->
					<Add after="cmd /c if exist ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\codesnippets.zip del ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\codesnippets.zip" />
					<Add after="zip -jq9 ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\codesnippets.zip resources/manifest.xml" />
					<!-- copy PNG files  -->
					<Add after="cmd /c copy .\resources\*.png ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\codesnippets\ &gt; $CMD_NULL 2&gt; $CMD_NULL" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="CodeSnippets_Plugin" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DNO_LOGGING" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../src" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
			<Add directory="Search" />
			<Add directory="editor" />
			<Add directory="." />
		</Compiler>
		<ResourceCompiler>
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs std,aui,propgrid`" />
			<Add library="codeblocks" />
			<Add directory="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
		</Linker>
		<Unit filename="Makefile.am" />
		<Unit filename="codesnippets.cpp" />
		<Unit filename="codesnippets.h" />
		<Unit filename="codesnippetstreectrl.cpp" />
		<Unit filename="codesnippetstreectrl.h" />
		<Unit filename="codesnippetswindow.cpp" />
		<Unit filename="codesnippetswindow.h" />
		<Unit filename="menuidentifiers.h" />
		<Unit filename="resources\Makefile.am" />
		<Unit filename="resources\allsnippets.png" />
		<Unit filename="resources\category.png" />
		<Unit filename="resources\findf.png" />
		<Unit filename="resources\findfdisabled.png" />
		<Unit filename="resources\manifest.rc" >
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="resources\manifest.xml" />
		<Unit filename="resources\options.png" />
		<Unit filename="resources\optionsdisabled.png" />
		<Unit filename="resources\snippet.png" />
		<Unit filename="resources\stop.png" />
		<Unit filename="resources\stopdisabled.png" />
		<Unit filename="settingsdlg.cpp" />
		<Unit filename="settingsdlg.h" />
		<Unit filename="settingsdlgform.cpp" />
		<Unit filename="settingsdlgform.h" />
		<Unit filename="snippetitemdata.cpp" />
		<Unit filename="snippetitemdata.h" />
		<Unit filename="snippetproperty.cpp" />
		<Unit filename="snippetproperty.h" />
		<Unit filename="snippetpropertyform.cpp" />
		<Unit filename="snippetpropertyform.h" />
		<Unit filename="snippetsconfig.cpp" />
		<Unit filename="snippetsconfig.h" />
		<Unit filename="snippetsimages.cpp" />
		<Unit filename="snippetsimages.h" />
		<Unit filename="version.cpp" />
		<Unit filename="version.h" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
