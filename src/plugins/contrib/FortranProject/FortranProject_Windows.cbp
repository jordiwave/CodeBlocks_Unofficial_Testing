<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FortranProject Windows" />
		<Option compiler="gcc" />
		<Option platforms="Windows;" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\plugins\FortranProject" prefix_auto="0" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)\.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\plugins\FortranProject" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<!-- Create output directory if it does not exist -->
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />

					<Add after="cmd /c if exist $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip del $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\FortranProject.zip" />
					<Add after="zip -qj9  $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip manifest.xml resources/*.xrc" />
					<Add after="zip -qr9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\$(TARGET_OUTPUT_BASENAME).zip ./images" />

					<Add after="cmd /c if not exist $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject\ mkdir $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject" />
					<Add after="xcopy  /Y /S /Q images\fortranproject\*.png $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject\" />
					<Add after="xcopy  /Y /S /Q images\fortranproject\*.dem $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject\" />
					<Add after="xcopy  /Y /S /Q images\fortranproject\*.f90 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject\" />
					<Add after="xcopy  /Y /S /Q images\fortranproject\*.py  $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\images\fortranproject\" />

					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-fno-keep-inline-dllexport" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
			<Add directory="$(#CB_BUILD.INCLUDE)\tinyxml" />
			<Add directory="$(#CB_BUILD)\sdk\wxscintilla\include" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)\msw$(#WXWIDGETS.WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS)/lib/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeblocks" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.lib)\gcc_dll$(#WXWIDGETS.WX_CFG)" />
		</Linker>
