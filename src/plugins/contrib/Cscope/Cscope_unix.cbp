<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Cscope Unix" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/Cscope" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/Cscope" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --cflags`" />
					<Add option="-Winvalid-pch" />
					<Add option="-fexceptions" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DcbDEBUG" />
					<Add directory="$(#CB_BUILD.INCLUDE)" />
					<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
					<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`$(#WXWIDGETS.WX_CONFIG_UNIX) --libs`" />
					<Add option="-Wl,--no-undefined" />
					<Add library="codeblocks" />
					<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -jq9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/Cscope.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
		</Compiler>
		<Unit filename="CscopeConfig.cpp" />
		<Unit filename="CscopeConfig.h" />
		<Unit filename="CscopeConfigPanel.cpp" />
		<Unit filename="CscopeConfigPanel.h" />
		<Unit filename="CscopeEntryData.cpp" />
		<Unit filename="CscopeEntryData.h" />
		<Unit filename="CscopeParserThread.cpp" />
		<Unit filename="CscopeParserThread.h" />
		<Unit filename="CscopePlugin.cpp" />
		<Unit filename="CscopePlugin.h" />
		<Unit filename="CscopeProcess.cpp" />
		<Unit filename="CscopeProcess.h" />
		<Unit filename="CscopeStatusMessage.cpp" />
		<Unit filename="CscopeStatusMessage.h" />
		<Unit filename="CscopeTab.cpp" />
		<Unit filename="CscopeTab.h" />
		<Unit filename="CscopeView.cpp" />
		<Unit filename="CscopeView.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/CscopeConfigPanel.wxs" />
		<Extensions>
			<lib_finder disable_auto="1" />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/CscopeConfigPanel.wxs" src="CscopeConfigPanel.cpp" hdr="CscopeConfigPanel.h" fwddecl="0" i18n="1" name="CscopeConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
