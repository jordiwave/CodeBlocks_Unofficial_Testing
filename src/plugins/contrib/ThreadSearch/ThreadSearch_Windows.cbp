<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocks_project_file>	<FileVersion major="1" minor="6" />	<Project>		<Option title="ThreadSearch Windows" />		<Option default_target="ThreadSearch" />		<Option compiler="gcc" />		<Build>			<Target title="ThreadSearch">				<Option output="../../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/ThreadSearch.dll" prefix_auto="0" extension_auto="0" />				<Option object_output="../../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/ThreadSearch" />				<Option type="3" />				<Option compiler="gcc" />				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />				<Option host_application="..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\codeblocks.exe" />				<Option run_host_application_in_terminal="0" />				<ExtraCommands>					<!-- Create output directory -->					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />					<Mode before="always" />										<Add after="zip ..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\ThreadSearch.zip manifest.xml" />					<Add after="cmd /c &quot;cd resources &amp;&amp; zip -rq9 ..\..\..\..\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\ThreadSearch.zip images&quot;" />					<Mode after="always" />				</ExtraCommands>			</Target>		</Build>		<VirtualTargets>			<Add alias="All" targets="ThreadSearch;" />		</VirtualTargets>		<Compiler>			<Add option="-Wall" />			<Add option="-pipe" />			<Add option="-mthreads" />			<Add option="-fmessage-length=0" />			<Add option="-fexceptions" />			<Add option="-fpermissive" />			<Add option="-DWXUSINGDLL" />			<Add option="-DHAVE_W32API_H" />			<Add option="-D__WXMSW__" />			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />			<Add option="-DcbDEBUG" />			<Add option="-Winvalid-pch" />			<Add option="-DCB_PRECOMP" />			<Add option="-DwxUSE_UNICODE" />			<Add option="