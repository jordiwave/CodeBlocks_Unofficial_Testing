<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbDisplayEvents MacOS" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/libDisplayEvents.so" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/DisplayEvents" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni" />
				<Option host_application="../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-std=$(#CB_BUILD.CPP_STD)" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG) --cflags`" />
					<Add option="-Winvalid-pch" />
					<Add option="-fexceptions" />
					<Add option="-fPIC" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DcbDEBUG" />
					<Add directory="../../include" />
				</Compiler>
				<Linker>
					<Add option="`$(#WXWIDGETS.WX_CONFIG) --libs`" />
					<Add library="codeblocks" />
					<Add directory="../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -jq9 ../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/DisplayEvents.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="DisplayEvents.cpp" />
		<Unit filename="DisplayEvents.h" />
		<Unit filename="Makefile.am" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
