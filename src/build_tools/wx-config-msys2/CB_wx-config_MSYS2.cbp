<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MSYS 2 wx-config-msys2" />
		<Option default_target="wx-config-msys2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="wx-config-msys2">
				<Option output="../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/wx-config-msys2" prefix_auto="0" extension_auto="1" />
				<Option working_dir="../../devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output="../../.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/tools/wx-config-msys2" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="1" />
				<Compiler>
					<Add option="-std=c++20" />
				</Compiler>
				<ExtraCommands>
					<!-- Create output directory -->
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />
					<Mode before="always" />
					<!-- Copy MSYS2 DLL files -->
					<Add after="$CMD_CP &quot;$(TARGET_COMPILER_DIR)bin\libgcc_s_seh-1.dll&quot;  $(TARGET_OUTPUT_DIR) &gt; $CMD_NULL 2&gt; $CMD_NULL" />
					<Add after="$CMD_CP &quot;$(TARGET_COMPILER_DIR)bin\libstdc++-6.dll&quot;     $(TARGET_OUTPUT_DIR) &gt; $CMD_NULL 2&gt; $CMD_NULL" />
					<Add after="$CMD_CP &quot;$(TARGET_COMPILER_DIR)bin\libwinpthread-1.dll&quot; $(TARGET_OUTPUT_DIR) &gt; $CMD_NULL 2&gt; $CMD_NULL" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="wx-config-msys2;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
		</Compiler>
		<Linker>
			<Add option="-s" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
		</Linker>
		<Unit filename="src/utils.cpp" />
		<Unit filename="src/utils.hpp" />
		<Unit filename="src/wx-config-msys2.cpp" />
		<Unit filename="src/wx-config.cpp">
			<Option compile="0" />
			<Option link="0" />
		</Unit>
		<Extensions>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
